<div class="scrollable-events col-sm-8 d-none d-lg-block">
    {{#each events as |event|}}
        {{#if (eq event.eventName displayEvent)}}
            {{{desc-helper event.eventDescription}}}
            {{#if (eq event.eventRegLink "None")}}
            {{else}}
                <a href={{event.eventRegLink}}>Register</a>
            {{/if}}
        {{/if}}
    {{/each}}
</div> 